<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="BOTTOM_CENTER" prefHeight="113.0" prefWidth="532.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Slider fx:id="progressBar">
            <VBox.margin>
                <Insets left="10.0" right="10.0" />
            </VBox.margin>
        </Slider>
        <ButtonBar prefHeight="40.0" prefWidth="402.0">
            <buttons>
                <Button fx:id="playBtn" mnemonicParsing="false" onAction="#play" prefWidth="515.0" text="Play" />
                <Button fx:id="pauseBtn" mnemonicParsing="false" onAction="#pause" text="Pause" />
                <Button fx:id="stopBtn" mnemonicParsing="false" onAction="#stop" prefHeight="25.0" text="Stop" textAlignment="JUSTIFY" />
                <Button fx:id="minus10Btn" mnemonicParsing="false" onAction="#minus10" text="-10s" textAlignment="JUSTIFY" />
                <Button fx:id="plus10Btn" mnemonicParsing="false" onAction="#plus10" text="+10s" textAlignment="JUSTIFY" />
                <Slider fx:id="volumeSlider" />
            </buttons>
            <padding>
                <Insets right="10.0" />
            </padding>
        </ButtonBar>
    </children>
</VBox>
